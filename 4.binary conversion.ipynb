    "# code for converting binary string into octal and hexa decimal conversion\n",
    "# wuestion\n",
    "bv=input(\"input the binary value preceded with 0B:\")\n",
    "iv=int(bv,2)\n",
    "ov=oct(iv)\n",
    "hv=hex(iv)\n",
    "print(\"oct({})={}\".format(bv,ov))\n",
    "print(\"hex({})={}\".format(bv,hv))"
   ]
